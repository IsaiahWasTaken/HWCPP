#include <iostream>
using namespace std;
int main(void) {
	int i = 0;
	
	for (i = 0; i < 101; i++) {
		if (((i % 3) == 0) && ((i % 5) != 0)) {
			cout << "fizz \n";
		}
		else if (((i % 5) == 0) && ((i % 3) != 0)) {
			cout << "buzz \n";
		}
		else if (((i % 5) == 0) && (i % 3) == 0 && (i !=0)) {
			cout << "FizzBuzz \n";
		}
		else {
			cout << i <<"\n";
		}
	}
	return 0;
}
